  "scripts": {
    "test": "jest",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\"",
    "start": "node server",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "build:clean": "del \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles --all -u 1 public/* 'public/**/*' dist",
    "build:dependencies": "webpack --profile --json > dependencies1.json",
    "prebuild": "npm run build:clean && npm run build:copy",
    "precommit": "lint-staged",
    "build": "NODE_ENV=\"production\" webpack -p --progress --colors",
    "build:scripts": "babel src/scripts --out-dir scripts/ --source-maps inline",
    "build:scripts:clean": "del \"scripts/*\"",
    "dev:scripts": "babel src/scripts/*.js --out-dir scripts/ --source-maps inline -w"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "fixtures"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
